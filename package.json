{
  "name": "evo-nuxt-bootstrap",
  "version": "1.0.0",
  "description": "Contenful, Tailwind, Nuxt Accelerator",
  "author": "Andy Pimlett",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint"
